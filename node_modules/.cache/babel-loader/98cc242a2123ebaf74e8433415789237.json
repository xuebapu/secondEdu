         <div class=\"name\">\r\n          <span style=\"fontSize:20px;color:#fff;fontWeight:700;fontFamily:STFangsong\">中华 继教</span>\r\n          <br />\r\n          <span\r\n            style=\"fontStyle:italic;fontSize:15px;color:#fff;fontWeight:600;fontFamily:KaiTi\"\r\n          >CHINA CME</span>\r\n        </div>\r\n        <div class=\"course\">\r\n          <el-cascader\r\n            @change=\"handleChange\"\r\n            v-model=\"subjectId\"\r\n            :options=\"courseTree\"\r\n            :props=\"defaultProps\"\r\n            placeholder=\"学科分类\"\r\n          ></el-cascader>\r\n        </div>\r\n<!--        <div class=\"liveName\">-->\r\n<!--          <span>会议直播</span>-->\r\n<!--        </div>-->\r\n        <div class=\"search\">\r\n          <div>\r\n            <el-dropdown @command=\"handleCommand1\">\r\n              <span class=\"el-dropdown-link\">\r\n                {{type}}\r\n                <i class=\"el-icon-arrow-down el-icon--right\"></i>\r\n              </span>\r\n              <el-dropdown-menu slot=\"dropdown\">\r\n                <el-dropdown-item command=\"课程\">课程</el-dropdown-item>\r\n                <el-dropdown-item command=\"直播\">直播</el-dropdown-item>\r\n              </el-dropdown-menu>\r\n            </el-dropdown>\r\n            <input type=\"text\" v-model=\"name\" />\r\n            <button @click=\"search\">\r\n              <i class=\"el-icon-search\"></i> 搜索\r\n            </button>\r\n          </div>\r\n        </div>\r\n        <!-- <div class=\"tip\">\r\n          <span></span>\r\n        </div>\r\n        <div class=\"shop\">\r\n          <span></span>\r\n        </div>-->\r\n        <div class=\"user\">\r\n          <el-breadcrumb style=\"lineHeight:2\" v-if=\"!token\" separator=\"/\">\r\n            <el-breadcrumb-item>\r\n              <span style=\"cursor:pointer\" @click=\"sign\">注册</span>\r\n            </el-breadcrumb-item>\r\n            <el-breadcrumb-item>\r\n              <span\r\n                style